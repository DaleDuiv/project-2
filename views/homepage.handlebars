{{#each exercises as |exercise|}}
<div class="container">
  <div class="container-left">
    <p>add exercise list here</p>
    <ul>
      <li>Squats</li>
      <li>add another exercise</li>

    </ul>
  </div>
  {{/each}}
 <!-- Button trigger modal -->
<button type="button" class="btn btn-primary" data-toggle="modal" data-target="#exampleModal" id="001">
  Launch demo modal
</button>

<!-- Modal -->
{{#if }}
<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Modal title</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        ...
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary">Save changes</button>
      </div>
    </div>
  </div>
</div>
{{/if}}
  {{#each sets as |set|}} 
  <div class="container-right">
    <h2>Current exercise</h2>
    <span>{{set.day}}</span>

  </div>
  {{/each}}
</div>





<script type="text/javascript" src="../js/newExercise.js"></script>
